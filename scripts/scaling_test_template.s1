-Seed "Test2025"

-read   +nodefile "%PATH%/input/synthetic/%FNAME%_nodes.tsv"
        +edgefile "%PATH%/input/synthetic/%FNAME%_edges.tsv"
        +arg name "%NAME%"
        +arg AUTHOR "%AUTHOR%"
        +arg LICENSE "%LICENSE%"
-save   "%PATH%/output/%FNAME%_model.m1"


-generate   "%PATH%/output/%FNAME%_nodes_x1.tsv" "%PATH%/output/%FNAME%_edges_x1.tsv" 1

-Scale 2
-generate   "%PATH%/output/%FNAME%_nodes_x2.tsv" "%PATH%/output/%FNAME%_edges_x2.tsv" 1

-Scale 5
-generate   "%PATH%/output/%FNAME%_nodes_x10.tsv" "%PATH%/output/%FNAME%_edges_x10.tsv" 1

-Scale 5
-generate   "%PATH%/output/%FNAME%_nodes_x50.tsv" "%PATH%/output/%FNAME%_edges_x50.tsv" 1


-load   "%PATH%/output/%FNAME%_model.m1"

-Scale 0.5
-generate   "%PATH%/output/%FNAME%_nodes_x05.tsv" "%PATH%/output/%FNAME%_edges_x05.tsv" 1

-Scale 0.5
-generate   "%PATH%/output/%FNAME%_nodes_x025.tsv" "%PATH%/output/%FNAME%_edges_x025.tsv" 1

